{
  "name": "11ty-template",
  "version": "1.0.0",
  "description": "A production-ready 11ty template with PagesCMS, GrapesJS, AlpineJS, and optimized build configuration",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npx @11ty/eleventy --serve",
    "build": "npx @11ty/eleventy",
    "sst:dev": "sst dev",
    "sst:deploy": "sst deploy",
    "sst:deploy:prod": "sst deploy --stage production",
    "sst:remove": "sst remove"
  },
  "keywords": [
    "eleventy",
    "11ty",
    "template",
    "static-site",
    "jamstack",
    "alpinejs",
    "pagescms",
    "grapesjs"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/trybettercraft/11ty-template.git"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-rss": "^2.0.2",
    "@quasibit/eleventy-plugin-sitemap": "^2.2.0",
    "alpinejs": "^3.14.1",
    "eleventy-plugin-llms-txt": "^1.2.0",
    "html-minifier-terser": "^7.2.0",
    "js-yaml": "^4.1.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@tailwindcss/typography": "^0.5.19",
    "aws-cdk-lib": "^2.220.0",
    "cssnano": "^7.1.2",
